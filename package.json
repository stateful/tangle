    "@types/vscode": "^1.61.0",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "eslint": "^8.1.0",
    "mocha": "^9.1.3",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.58.3",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-terser": "^7.0.2",
