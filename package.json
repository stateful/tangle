{
  "name": "vscoderx",
  "displayName": "vscoderx",
  "description": "Minimal state-management implementation to connect insular views in VS Code",
  "publisher": "activecove",
  "version": "0.1.2",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "types": "dist/vrx.d.ts",
  "main": "dist/vrx.cjs.js",
  "module": "dist/vrx.esm.js",
  "exports": {
    "require": "./dist/vrx.cjs.js",
    "import": "./dist/vrx.esm.js"
  },
  "scripts": {
    "compile": "rollup -c ./rollup.config.js",
    "clean": "rm -rf dist",
    "watch": "webpack --watch --mode development",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "yarn run test-compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./dist/test/runTest.js"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@types/node": "16.11.6",
    "@types/vscode": "^1.61.0",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "eslint": "^8.1.0",
    "mocha": "^9.1.3",
    "rollup": "^2.58.3",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@rollup/plugin-typescript": "^8.3.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "rxjs": "^7.4.0"
  }
}
