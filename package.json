  "license": "Apache-2.0",
  "contributors": [
    "christian-bromann <christian@stateful.com>"
  ],
  "engines": {
    "node": ">=12.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/stateful/vscoderx.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/stateful/vscoderx/issues"
  },
  "types": "dist/vrx.d.ts",
  "main": "dist/vrx.cjs.js",
  "module": "dist/vrx.esm.js",
  "exports": {
